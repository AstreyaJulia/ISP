{
  "name": "react-app",
  "version": "0.1.0",
  "author": "Latysheva Y. A., Chernov R. A.",
  "license": "ISC",
  "description": "Информационно-справочная система.",
  "private": true,
  "packageManager": "yarn@4.0.1",
  "scripts": {
    "start:app-development": "react-scripts start",
    "start:storybook": "storybook dev -p 6006 -s public",
    "build:app": "react-scripts build",
    "start:app-build": "serve -s build -l 3000",
    "build:storybook": "storybook build -s public",
    "test": "react-scripts test",
    "clean:yarn-cache": "yarn cache clean",
    "format": "yarn prettier . --write",
    "eslint": "eslint --ext .js,.jsx ./src",
    "eslint:fix": "eslint --fix --ext .js,.jsx ./src ./tailwind.config.js ./.storybook",
    "upgrade modules": "yarn upgrade-interactive",
    "update modules": "yarn up",
    "install modules": "yarn install",
    "deduplicate dependencies": "yarn dedupe"
  },
  "dependencies": {
